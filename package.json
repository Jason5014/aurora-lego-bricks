    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.6.2",
    "vue": "^3.0.0"
